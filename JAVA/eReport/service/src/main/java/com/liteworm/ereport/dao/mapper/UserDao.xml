<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
       <!-- //注意这个namespace位置-->
<mapper namespace="com.liteworm.ereport.dao">
<!-- //注意这个resultType类型-->
<select id="selAll" resultType="com.liteworm.ereport.entity.User">
	        select * from user
	    </select>

<insert id="insertUser">
	        insert user (name,age) value (#{name},#{age})
	</insert>
</mapper>